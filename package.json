{
  "name": "reactfundamentals",
  "version": "1.0.0",
  "description": "React Fundamentals course",
  "main": "index.js",
  "scripts": {
    "production": "webpack -p",
    "start": "webpack-dev-server",
    "test": "jest --coverage --no-cache|| true"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^2.7.1",
    "@types/history": "^2.0.45",
    "@types/jest": "^16.0.4",
    "@types/node": "^7.0.0",
    "@types/react": "^0.14.55",
    "@types/react-addons-css-transition-group": "^0.14.19",
    "@types/react-addons-test-utils": "^0.14.16",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.44",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "html-webpack-plugin": "^2.24.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "lodash": "^4.17.4",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-jest": "^18.0.1",
    "tslint": "^4.2.0",
    "tslint-loader": "^3.3.0",
    "tslint-react": "^2.2.0",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
